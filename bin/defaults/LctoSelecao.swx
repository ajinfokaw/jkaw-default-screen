eguro_valor,icmssubst_valor,icmssubst_basecalculo,dataentradasaida,fat_serie.seriemodelo AS seriemodelo, (select propriedade FROM view_transacao WHERE codigo_transacao = fat_docto_c.fat_transacao_id AND diretiva = 122) AS operacao FROM fat_docto_c LEFT JOIN cad_cadastro ON cad_cadastro.id = fat_docto_c.cad_cadastro_id LEFT JOIN fat_transacao ON fat_transacao.id = fat_docto_c.fat_transacao_id LEFT JOIN fat_serie ON fat_serie.id = fat_docto_c.seriedocto_id WHERE CASE WHEN (COALESCE(:pcontrole,0) > 0) THEN controle = :pcontrole WHEN (COALESCE(:pnumero_docto,0) > 0) THEN numero_docto = :pnumero_docto WHEN (COALESCE(:pcliente,0) > 0) THEN fat_docto_c.cad_cadastro_id = :pcliente AND emissao BETWEEN CAST(COALESCE(CAST(:datainicial AS VARCHAR(10)),CAST(CURRENT_DATE AS VARCHAR)) AS DATE) AND CAST(COALESCE(CAST(:datafinal AS VARCHAR(10)),CAST(CURRENT_DATE AS VARCHAR)) AS DATE) WHEN (COALESCE(:ptransacao,0) > 0) THEN fat_docto_c.fat_transacao_id = :ptransacao AND emissao BETWEEN CAST(COALESCE(CAST(:datainicial AS VARCHAR(10)),CAST(CURRENT_DATE AS VARCHAR)) AS DATE) AND CAST(COALESCE(CAST(:datafinal AS VARCHAR(10)),CAST(CURRENT_DATE AS VARCHAR)) AS DATE) ELSE emissao BETWEEN CAST(COALESCE(CAST(:datainicial AS VARCHAR(10)),CAST(CURRENT_DATE AS VARCHAR)) AS DATE) AND CAST(COALESCE(CAST(:datafinal AS VARCHAR(10)),CAST(CURRENT_DATE AS VARCHAR)) AS DATE) END AND CASE WHEN coalesce(TRIM(:transacoes_habilitadas), '') = '' THEN fat_docto_c.fat_transacao_id in(select fat_transacao.id from fat_transacao) ELSE fat_docto_c.fat_transacao_id = any(CAST(regexp_split_to_array(:transacoes_habilitadas,',') AS int[])) END AND CASE WHEN (NOT(:super_user) AND :vendedor_id>-1) THEN fat_docto_c.cadastro_vendedor_id = :vendedor_id ELSE COALESCE(fat_docto_c.cadastro_vendedor_id, 0)>-1 END AND COALESCE(:ppesquisa,' ')=' ' ORDER BY emissao DESC, controle DESC LIMIT :reg_p_pagina OFFSET(:pagina - 1) * :reg_p_pagina" queryDataSetView="Select count(fat_docto_c.controle)/CAST(:reg_p_pagina AS DOUBLE PRECISION) AS paginas from fat_docto_c WHERE CASE WHEN (COALESCE(:pcontrole,0) > 0) THEN controle = :pcontrole WHEN (COALESCE(:pnumero_docto,0) > 0) THEN numero_docto = :pnumero_docto WHEN (COALESCE(:pcliente,0) > 0) THEN fat_docto_c.cad_cadastro_id = :pcliente AND emissao BETWEEN CAST(COALESCE(CAST(:datainicial AS VARCHAR(10)),CAST(CURRENT_DATE AS VARCHAR)) AS DATE) AND CAST(COALESCE(CAST(:datafinal AS VARCHAR(10)),CAST(CURRENT_DATE AS VARCHAR)) AS DATE) WHEN (COALESCE(:ptransacao,0) > 0) THEN fat_docto_c.fat_transacao_id = :ptransacao AND emissao BETWEEN CAST(COALESCE(CAST(:datainicial AS VARCHAR(10)),CAST(CURRENT_DATE AS VARCHAR)) AS DATE) AND CAST(COALESCE(CAST(:datafinal AS VARCHAR(10)),CAST(CURRENT_DATE AS VARCHAR)) AS DATE) ELSE emissao BETWEEN CAST(COALESCE(CAST(:datainicial AS VARCHAR(10)),CAST(CURRENT_DATE AS VARCHAR)) AS DATE) AND CAST(COALESCE(CAST(:datafinal AS VARCHAR(10)),CAST(CURRENT_DATE AS VARCHAR)) AS DATE) END AND CASE WHEN coalesce(TRIM(:transacoes_habilitadas), '') = '' THEN fat_docto_c.fat_transacao_id in(select fat_transacao.id from fat_transacao) ELSE fat_docto_c.fat_transacao_id = any(CAST(regexp_split_to_array(:transacoes_habilitadas,',') AS int[])) END AND CASE WHEN (NOT(:super_user) AND :vendedor_id>-1) THEN fat_docto_c.cadastro_vendedor_id = :vendedor_id ELSE COALESCE(fat_docto_c.cadastro_vendedor_id, 0)>-1 END AND COALESCE(:ppesquisa,' ')=' '  "/>
